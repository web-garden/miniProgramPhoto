button::after{
  border: none
} 
.clearfix {
  zoom: 1;
}
.clearfix:before, .clearfix:after {
  display: table;
  content: "";
  line-height: 0;
  clear: both;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.none{
  visibility: hidden;
}
.cuttext{
  display: block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space:nowrap;
}
image{
  display: block;
}
.nav_title{
  margin:10px 0;
  display: flex;
  flex-direction:row;
  justify-content: space-between;
  padding:0 5px;
  height:18px;
  line-height: 18px;
  font-size: 14px;
}
.nav_tit{
  border-left: 3px solid #f37121;
  padding-left:10px;
  height:18px;
  line-height: 18px;
  font-size: 14px;
}